<!DOCTYPE html>
<html{{__pyjade_attrs(terse=True, attrs=[('lang',(""+"{}".format(DEFAULT_LANG)+""))])}}>
  <head>{% block head %}{% endblock %}
    <title>{% block title %}{{SITENAME|escape}}{% endblock %}
    </title>
    <meta charset="utf-8">
  </head>
  <body id="index" class="home">
    <header id="banner" class="body">
      <h1><a{{__pyjade_attrs(terse=True, attrs=[('href',(""+"{}".format(SITEURL)+"/"))])}}>{{SITENAME|escape}}<strong>{{SITESUBTITLE|escape}}</strong></a>
      </h1>
    </header>
    <nav id="memu">
      <ul>{% for title,link in __pyjade_iter(MENUITEMS,2) %}
        <li><a{{__pyjade_attrs(terse=True, attrs=[('href',(""+"{}".format(link)+""))])}}>{{title|escape}}</a>
        </li>{% endfor %}{% if  DISPLAY_PAGES_ON_MENU %}{% for p in __pyjade_iter(PAGES,1) %}{% if  p == page %}
        <li class="active"></li>{% else %}
        <li></li>{% endif %}<a{{__pyjade_attrs(terse=True, attrs=[('href',(""+"{}".format(SITEURL)+"/"+"{}".format(p.url)+""))])}}>{{p.title|escape}}</a>{% endfor %}{% else %}{% if  DISPLAY_CATEGORIES_ON_MENU %}{% for cat,null in __pyjade_iter(categories,2) %}{% if   %}
        <li class="active"></li>{% else %}
        <li></li>{% endif %}<a{{__pyjade_attrs(terse=True, attrs=[('href',(""+"{}".format(SITEURL)+"/"+"{}".format(cat.url)+""))])}}>{{cat|escape}}</a>{% endfor %}{% endif %}{% endif %}
      </ul>
    </nav>{% block content %}{% endblock %}
    <fotter id="contentinfo" class="body">
      <address id="about" class="vcard body">Proudly powered by<a href="http://getpelican.com/">Pelican</a>
      </address>
    </fotter>
  </body>
</html>
